{
  "name": "@xplart/pow-frontend",
  "scripts": {
    "hardhat": "set NODE_OPTIONS=--openssl-legacy-provider && hardhat",
    "build": "rollup -c",
    "start-dev": "concurrently -k -n hardhat,rollup,http \"npm:hardhat node\" \"rollup -c -w\" \"http-server\"",
    "deploy-contract-local": "hardhat run ./node_modules/@xplart/pow-onchain/scripts/deploy.ts --network localhost"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/lodash": "^4.14.178",
    "@xplart/pow-onchain": "../pow-onchain",
    "concurrently": "^7.0.0",
    "http-server": "^14.1.0",
    "rollup": "^2.67.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "ethers": "^5.5.4",
    "hardhat": "^2.8.4",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "preact": "^10.6.5",
    "rxjs": "^7.5.4"
  }
}
